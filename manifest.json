{
	"manifest_version":2,
	"name":"Bomerang URL",
	"description": "Reminder to go back to this URL in the future",
	"version":"0.1",
	"content_scripts":[
		{
			"matches":[
				"<all_urls>"
			],
			"js":[
				"jquery-2.2.3.min.js"
			]
		}
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Boomerang URL"
	},
	"permissions": [
		"tabs", "http://*/*", "https://*/*"
	]
}